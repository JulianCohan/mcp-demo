      "Bash(fi)",
      "Bash(done)",
      "Bash(for class in \"example-card\" \"code-tabs\" \"tab-button\" \"tab-content\" \"code-showcase\")",
      "Bash(then echo \"USED - KEEP\")",
      "Bash(else echo \"NOT USED\")",
